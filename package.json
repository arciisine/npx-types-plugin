{
  "name": "npx-scripts",
  "displayName": "npx scripts",
  "description": "General support for npx scripts, with typings via npx shebang and execution",
  "version": "0.1.7",
  "homepage": "https://github.com/arciisine/vscode-npx-scripts",
  "publisher": "arcsine",
  "keywords": [
    "vscode",
    "scripts",
    "npx",
    "shebang"
  ],
  "author": {
    "email": "timothy.soehnlin@gmail.com",
    "name": "Timothy Soehnlin"
  },
  "repository": {
    "url": "https://github.com/arciisine/vscode-npx-scripts.git"
  },
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "files": [
    "src/"
  ],
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "contributes": {
    "commands": [
      {
        "title": "Run npx script",
        "category": "npx",
        "command": "npx-scripts.run",
        "when": "editorLangId == javascript && !isInDiffEditor"
      },
      {
        "title": "Re-install package",
        "category": "npx",
        "command": "npx-scripts.re-install",
        "when": "editorLangId == javascript  && !isInDiffEditor"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^11.13.2",
    "vscode": "^1.1.33"
  },
  "icon": "images/logo.png",
  "galleryBanner": {
    "color": "#c6cdd3",
    "theme": "light"
  }
}